# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'Toast' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Toast
  pod 'MDFoundation', :git => 'https://github.com/NGdev1/MDFoundation.git'
  pod 'Moya', '~> 14.0'
  pod 'SnapKit', '~> 5.0'
  pod 'SPAlert'
  pod 'KeychainSwift'
  pod 'SwiftGen'qx
end
